{
  "name": "GymSheet",
  "version": "1.0.0",
  "description": "A gym-tracking web application",
  "scripts": {
    "start:client": "webpack-dev-server --mode development",
    "start:server": "nodemon --exec babel-node server/src/app.js",
    "build": "webpack --mode production && babel ./server/src --out-dir ./server/dist --delete-dir-on-start",
    "postinstall": "npm run build",
    "start": "node server/dist/app.js",
    "flow": "flow",
    "db:init": "db-migrate db:create gym --env=dev-init --config db/database.json",
    "db:drop": "db-migrate db:drop gym --env=dev-init --config db/database.json",
    "db:up": "db-migrate up --config db/database.json -m db/migrations",
    "db:reset": "db-migrate reset --config db/database.json -m db/migrations",
    "db:up:production": "DATABASE_URL=$(heroku config:get DATABASE_URL -a rosalia-gym-sheet)?ssl=true npm run db:up"
  },
  "author": "Rachel Rosalia <rachel@rosalia.me>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "db-migrate": "^0.11.5",
    "db-migrate-pg": "^0.4.0",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-flowtype-errors": "^3.6.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.91.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.16.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.2.5",
    "@babel/runtime": "^7.3.0",
    "bcrypt": "^3.0.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "massive": "^5.7.3",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1"
  }
}
